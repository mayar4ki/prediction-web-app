// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db/dev.db"
}

model Round {
  id       Int     @id @default(autoincrement())
  roundId  BigInt
  hasPhoto Boolean
  tags     Tag[]
}

model Tag {
  id     Int     @id @default(autoincrement())
  name   String
  rounds Round[]
}
